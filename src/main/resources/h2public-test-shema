CREATE TABLE CATALOGS
(
    CATALOG_NAME VARCHAR(100)
);
CREATE TABLE COLLATIONS
(
    NAME VARCHAR(100),
    KEY VARCHAR(100)
);
CREATE TABLE COLUMNS
(
    TABLE_CATALOG VARCHAR(100),
    TABLE_SCHEMA VARCHAR(100),
    TABLE_NAME VARCHAR(100),
    COLUMN_NAME VARCHAR(100),
    ORDINAL_POSITION INTEGER,
    COLUMN_DEFAULT VARCHAR(100),
    IS_NULLABLE VARCHAR(100),
    DATA_TYPE INTEGER,
    CHARACTER_MAXIMUM_LENGTH INTEGER,
    CHARACTER_OCTET_LENGTH INTEGER,
    NUMERIC_PRECISION INTEGER,
    NUMERIC_PRECISION_RADIX INTEGER,
    NUMERIC_SCALE INTEGER,
    CHARACTER_SET_NAME VARCHAR(100),
    COLLATION_NAME VARCHAR(100),
    TYPE_NAME VARCHAR(100),
    NULLABLE INTEGER,
    IS_COMPUTED BOOLEAN,
    SELECTIVITY INTEGER,
    CHECK_CONSTRAINT VARCHAR(100),
    SEQUENCE_NAME VARCHAR(100),
    REMARKS VARCHAR(100),
    SOURCE_DATA_TYPE SMALLINT
);
CREATE TABLE COLUMN_PRIVILEGES
(
    GRANTOR VARCHAR(100),
    GRANTEE VARCHAR(100),
    TABLE_CATALOG VARCHAR(100),
    TABLE_SCHEMA VARCHAR(100),
    TABLE_NAME VARCHAR(100),
    COLUMN_NAME VARCHAR(100),
    PRIVILEGE_TYPE VARCHAR(100),
    IS_GRANTABLE VARCHAR(100)
);
CREATE TABLE CONSTANTS
(
    CONSTANT_CATALOG VARCHAR(100),
    CONSTANT_SCHEMA VARCHAR(100),
    CONSTANT_NAME VARCHAR(100),
    DATA_TYPE INTEGER,
    REMARKS VARCHAR(100),
    SQL VARCHAR(100),
    ID INTEGER
);
CREATE TABLE CONSTRAINTS
(
    CONSTRAINT_CATALOG VARCHAR(100),
    CONSTRAINT_SCHEMA VARCHAR(100),
    CONSTRAINT_NAME VARCHAR(100),
    CONSTRAINT_TYPE VARCHAR(100),
    TABLE_CATALOG VARCHAR(100),
    TABLE_SCHEMA VARCHAR(100),
    TABLE_NAME VARCHAR(100),
    UNIQUE_INDEX_NAME VARCHAR(100),
    CHECK_EXPRESSION VARCHAR(100),
    COLUMN_LIST VARCHAR(100),
    REMARKS VARCHAR(100),
    SQL VARCHAR(100),
    ID INTEGER
);
CREATE TABLE CROSS_REFERENCES
(
    PKTABLE_CATALOG VARCHAR(100),
    PKTABLE_SCHEMA VARCHAR(100),
    PKTABLE_NAME VARCHAR(100),
    PKCOLUMN_NAME VARCHAR(100),
    FKTABLE_CATALOG VARCHAR(100),
    FKTABLE_SCHEMA VARCHAR(100),
    FKTABLE_NAME VARCHAR(100),
    FKCOLUMN_NAME VARCHAR(100),
    ORDINAL_POSITION SMALLINT,
    UPDATE_RULE SMALLINT,
    DELETE_RULE SMALLINT,
    FK_NAME VARCHAR(100),
    PK_NAME VARCHAR(100),
    DEFERRABILITY SMALLINT
);
CREATE TABLE DOMAINS
(
    DOMAIN_CATALOG VARCHAR(100),
    DOMAIN_SCHEMA VARCHAR(100),
    DOMAIN_NAME VARCHAR(100),
    COLUMN_DEFAULT VARCHAR(100),
    IS_NULLABLE VARCHAR(100),
    DATA_TYPE INTEGER,
    PRECISION INTEGER,
    SCALE INTEGER,
    TYPE_NAME VARCHAR(100),
    SELECTIVITY INTEGER,
    CHECK_CONSTRAINT VARCHAR(100),
    REMARKS VARCHAR(100),
    SQL VARCHAR(100),
    ID INTEGER
);
CREATE TABLE FUNCTION_ALIASES
(
    ALIAS_CATALOG VARCHAR(100),
    ALIAS_SCHEMA VARCHAR(100),
    ALIAS_NAME VARCHAR(100),
    JAVA_CLASS VARCHAR(100),
    JAVA_METHOD VARCHAR(100),
    DATA_TYPE INTEGER,
    TYPE_NAME VARCHAR(100),
    COLUMN_COUNT INTEGER,
    RETURNS_RESULT SMALLINT,
    REMARKS VARCHAR(100),
    ID INTEGER,
    SOURCE VARCHAR(100)
);
CREATE TABLE FUNCTION_COLUMNS
(
    ALIAS_CATALOG VARCHAR(100),
    ALIAS_SCHEMA VARCHAR(100),
    ALIAS_NAME VARCHAR(100),
    JAVA_CLASS VARCHAR(100),
    JAVA_METHOD VARCHAR(100),
    COLUMN_COUNT INTEGER,
    POS INTEGER,
    COLUMN_NAME VARCHAR(100),
    DATA_TYPE INTEGER,
    TYPE_NAME VARCHAR(100),
    PRECISION INTEGER,
    SCALE SMALLINT,
    RADIX SMALLINT,
    NULLABLE SMALLINT,
    COLUMN_TYPE SMALLINT,
    REMARKS VARCHAR(100),
    COLUMN_DEFAULT VARCHAR(100)
);
CREATE TABLE HELP
(
    ID INTEGER,
    SECTION VARCHAR(100),
    TOPIC VARCHAR(100),
    SYNTAX VARCHAR(100),
    TEXT VARCHAR(100)
);
CREATE TABLE INDEXES
(
    TABLE_CATALOG VARCHAR(100),
    TABLE_SCHEMA VARCHAR(100),
    TABLE_NAME VARCHAR(100),
    NON_UNIQUE BOOLEAN,
    INDEX_NAME VARCHAR(100),
    ORDINAL_POSITION SMALLINT,
    COLUMN_NAME VARCHAR(100),
    CARDINALITY INTEGER,
    PRIMARY_KEY BOOLEAN,
    INDEX_TYPE_NAME VARCHAR(100),
    IS_GENERATED BOOLEAN,
    INDEX_TYPE SMALLINT,
    ASC_OR_DESC VARCHAR(100),
    PAGES INTEGER,
    FILTER_CONDITION VARCHAR(100),
    REMARKS VARCHAR(100),
    SQL VARCHAR(100),
    ID INTEGER,
    SORT_TYPE INTEGER,
    CONSTRAINT_NAME VARCHAR(100),
    INDEX_CLASS VARCHAR(100)
);
CREATE TABLE IN_DOUBT
(
    TRANSACTION VARCHAR(100),
    STATE VARCHAR(100)
);
CREATE TABLE LOCKS
(
    TABLE_SCHEMA VARCHAR(100),
    TABLE_NAME VARCHAR(100),
    SESSION_ID INTEGER,
    LOCK_TYPE VARCHAR(100)
);
CREATE TABLE QUERY_STATISTICS
(
    SQL_STATEMENT VARCHAR(100),
    EXECUTION_COUNT INTEGER,
    MIN_EXECUTION_TIME BIGINT,
    MAX_EXECUTION_TIME BIGINT,
    CUMULATIVE_EXECUTION_TIME BIGINT,
    AVERAGE_EXECUTION_TIME DOUBLE,
    STD_DEV_EXECUTION_TIME DOUBLE,
    MIN_ROW_COUNT INTEGER,
    MAX_ROW_COUNT INTEGER,
    CUMULATIVE_ROW_COUNT BIGINT,
    AVERAGE_ROW_COUNT DOUBLE,
    STD_DEV_ROW_COUNT DOUBLE
);
CREATE TABLE RIGHTS
(
    GRANTEE VARCHAR(100),
    GRANTEETYPE VARCHAR(100),
    GRANTEDROLE VARCHAR(100),
    RIGHTS VARCHAR(100),
    TABLE_SCHEMA VARCHAR(100),
    TABLE_NAME VARCHAR(100),
    ID INTEGER
);
CREATE TABLE ROLES
(
    NAME VARCHAR(100),
    REMARKS VARCHAR(100),
    ID INTEGER
);
CREATE TABLE SCHEMATA
(
    CATALOG_NAME VARCHAR(100),
    SCHEMA_NAME VARCHAR(100),
    SCHEMA_OWNER VARCHAR(100),
    DEFAULT_CHARACTER_SET_NAME VARCHAR(100),
    DEFAULT_COLLATION_NAME VARCHAR(100),
    IS_DEFAULT BOOLEAN,
    REMARKS VARCHAR(100),
    ID INTEGER
);
CREATE TABLE SEQUENCES
(
    SEQUENCE_CATALOG VARCHAR(100),
    SEQUENCE_SCHEMA VARCHAR(100),
    SEQUENCE_NAME VARCHAR(100),
    CURRENT_VALUE BIGINT,
    INCREMENT BIGINT,
    IS_GENERATED BOOLEAN,
    REMARKS VARCHAR(100),
    CACHE BIGINT,
    MIN_VALUE BIGINT,
    MAX_VALUE BIGINT,
    IS_CYCLE BOOLEAN,
    ID INTEGER
);
CREATE TABLE SESSIONS
(
    ID INTEGER,
    USER_NAME VARCHAR(100),
    SESSION_START VARCHAR(100),
    STATEMENT VARCHAR(100),
    STATEMENT_START VARCHAR(100),
    CONTAINS_UNCOMMITTED VARCHAR(100)
);
CREATE TABLE SESSION_STATE
(
    KEY VARCHAR(100),
    SQL VARCHAR(100)
);
CREATE TABLE SETTINGS
(
    NAME VARCHAR(100),
    VALUE VARCHAR(100)
);
CREATE TABLE TABLES
(
    TABLE_CATALOG VARCHAR(100),
    TABLE_SCHEMA VARCHAR(100),
    TABLE_NAME VARCHAR(100),
    TABLE_TYPE VARCHAR(100),
    STORAGE_TYPE VARCHAR(100),
    SQL VARCHAR(100),
    REMARKS VARCHAR(100),
    LAST_MODIFICATION BIGINT,
    ID INTEGER,
    TYPE_NAME VARCHAR(100),
    TABLE_CLASS VARCHAR(100),
    ROW_COUNT_ESTIMATE BIGINT
);
CREATE TABLE TABLE_PRIVILEGES
(
    GRANTOR VARCHAR(100),
    GRANTEE VARCHAR(100),
    TABLE_CATALOG VARCHAR(100),
    TABLE_SCHEMA VARCHAR(100),
    TABLE_NAME VARCHAR(100),
    PRIVILEGE_TYPE VARCHAR(100),
    IS_GRANTABLE VARCHAR(100)
);
CREATE TABLE TABLE_TYPES
(
    TYPE VARCHAR(100)
);
CREATE TABLE TRIGGERS
(
    TRIGGER_CATALOG VARCHAR(100),
    TRIGGER_SCHEMA VARCHAR(100),
    TRIGGER_NAME VARCHAR(100),
    TRIGGER_TYPE VARCHAR(100),
    TABLE_CATALOG VARCHAR(100),
    TABLE_SCHEMA VARCHAR(100),
    TABLE_NAME VARCHAR(100),
    BEFORE BOOLEAN,
    JAVA_CLASS VARCHAR(100),
    QUEUE_SIZE INTEGER,
    NO_WAIT BOOLEAN,
    REMARKS VARCHAR(100),
    SQL VARCHAR(100),
    ID INTEGER
);
CREATE TABLE TYPE_INFO
(
    TYPE_NAME VARCHAR(100),
    DATA_TYPE INTEGER,
    PRECISION INTEGER,
    PREFIX VARCHAR(100),
    SUFFIX VARCHAR(100),
    PARAMS VARCHAR(100),
    AUTO_INCREMENT BOOLEAN,
    MINIMUM_SCALE SMALLINT,
    MAXIMUM_SCALE SMALLINT,
    RADIX INTEGER,
    POS INTEGER,
    CASE_SENSITIVE BOOLEAN,
    NULLABLE SMALLINT,
    SEARCHABLE SMALLINT
);
CREATE TABLE USERS
(
    NAME VARCHAR(100),
    ADMIN VARCHAR(100),
    REMARKS VARCHAR(100),
    ID INTEGER
);
CREATE TABLE VIEWS
(
    TABLE_CATALOG VARCHAR(100),
    TABLE_SCHEMA VARCHAR(100),
    TABLE_NAME VARCHAR(100),
    VIEW_DEFINITION VARCHAR(100),
    CHECK_OPTION VARCHAR(100),
    IS_UPDATABLE VARCHAR(100),
    STATUS VARCHAR(100),
    REMARKS VARCHAR(100),
    ID INTEGER
);
CREATE TABLE ADDRESS
(
    ID INTEGER PRIMARY KEY NOT NULL,
    CITY VARCHAR(20),
    HOME VARCHAR(4),
    FLAT VARCHAR(10),
    DELETED INTEGER
);
CREATE TABLE CLIENT_ORDER
(
    ID INTEGER PRIMARY KEY NOT NULL,
    NUMBER VARCHAR(10) NOT NULL,
    USER INTEGER NOT NULL,
    INSERT_DATE DATE,
    DELETED INTEGER,
    SUM_PAID DECIMAL(10,2)
);
CREATE TABLE ORDER_DETAIL
(
    CLIENT_ORDER INTEGER NOT NULL,
    QUANTITY INTEGER,
    PRODUCT INTEGER,
    SUM DECIMAL(10,2),
    SUM_PAID DECIMAL(10,2),
    DELETED INTEGER
);
CREATE TABLE PRODUCT
(
    ID INTEGER PRIMARY KEY NOT NULL,
    NAME VARCHAR(50) NOT NULL,
    ACTIVE INTEGER,
    INSERT_DATE TIMESTAMP DEFAULT CURRENT_TIMESTAMP() NOT NULL,
    PRICE DOUBLE,
    DELETED INTEGER
);
CREATE TABLE ROLE
(
    ID INTEGER PRIMARY KEY NOT NULL,
    NAME VARCHAR(20) NOT NULL,
    ACTIVE INTEGER NOT NULL
);
CREATE TABLE USER
(
    ID INTEGER PRIMARY KEY NOT NULL,
    NAME VARCHAR(50) NOT NULL,
    ROLE INTEGER NOT NULL,
    ADDRESS INTEGER,
    LOGIN VARCHAR(50) NOT NULL,
    PASSWORD VARCHAR(10) NOT NULL,
    INN VARCHAR(12) NOT NULL,
    BIRTH_DAY DATE,
    BLACKLIST INTEGER,
    INSERT_DATE DATE NOT NULL,
    DELETED INTEGER
);
